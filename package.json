    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "4.12.1",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.18",
    "@mui/x-data-grid": "^6.18.2",
    "cypress-commands": "^1.1.0",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "qs": "^6.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "serve": "^11.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
